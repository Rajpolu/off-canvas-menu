{
  "name": "off-canvas-menu",
  "version": "1.0.0",
  "description": "Simple Vanilla JS Off-Canvas-Menu",
  "main": "gulpfile.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development npx webpack serve --config config/webpack.dev.js",
    "step1": "rm -R public && mkdir public && cd public && touch .gitkeep && cd ..",
    "step2": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js && gulp",
    "build": "npm run step1 && npm run step2"
  },
